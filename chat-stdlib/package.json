  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.8.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^9",
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "eslint": "9.x",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  }
